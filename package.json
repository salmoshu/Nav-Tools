{
  "name": "nav-tools",
  "version": "v0.2.11-20251019",
  "main": "dist-electron/main/index.js",
  "description": "Nav-Tools is a desktop-grade visualization workbench built with Electron & Vue3, tailored for roboticists.",
  "author": "Winchell.Hu <winchell.hu@outlook.com>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "chcp 65001 && vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "clean": "rimraf node_modules dist dist-electron release",
    "rebuild": "npm run clean && npm ci"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-monaco-editor-esm": "^2.0.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "echarts": "^6.0.0",
    "element-plus": "^2.11.2",
    "element-resize-detector": "^1.2.4",
    "grid-layout-plus": "^1.1.0",
    "mathjs": "^14.7.0",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.53.0",
    "pinia": "^3.0.3",
    "serialport": "^13.0.0"
  }
}
